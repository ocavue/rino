{
  "name": "rino",
  "version": "0.5.0",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export -o dist",
    "test": "scripts/test.sh",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint --fix -c .eslintrc.yaml --ignore-path .gitignore --ext '.js,.jsx,ts,tsx' .",
    "coverage": "cat ./coverage/lcov.info | COVERALLS_REPO_TOKEN=$COVERALLS_REPO_TOKEN coveralls"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "postUpdateOptions": [
      "yarnDedupeHighest"
    ],
    "lockFileMaintenance": {
      "enabled": true
    },
    "packageRules": [
      {
        "depTypeList": [
          "dependencies"
        ],
        "updateTypes": [
          "patch",
          "minor"
        ],
        "groupName": "dependencies (non-major)"
      },
      {
        "depTypeList": [
          "devDependencies"
        ],
        "updateTypes": [
          "patch",
          "minor"
        ],
        "groupName": "devDependencies (non-major)"
      }
    ],
    "rangeStrategy": "update-lockfile"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@next/bundle-analyzer": "^9.2.0",
    "@remirror/core": "^0.9.0",
    "@remirror/core-extensions": "^0.7.3",
    "@remirror/dev": "^0.7.3",
    "@remirror/extension-code-block": "^0.7.4",
    "@remirror/react": "^0.7.4",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest": "^25.0.0",
    "@types/jest-environment-puppeteer": "^4.3.0",
    "@types/lodash": "^4.14.134",
    "@types/markdown-it": ">=0.0.7",
    "@types/node": "^12.0.0",
    "@types/prosemirror-commands": "^1.0.0",
    "@types/prosemirror-dropcursor": "^1.0.0",
    "@types/prosemirror-history": "^1.0.0",
    "@types/prosemirror-inputrules": "^1.0.0",
    "@types/prosemirror-keymap": "^1.0.0",
    "@types/prosemirror-model": "^1.0.0",
    "@types/prosemirror-schema-list": "^1.0.0",
    "@types/prosemirror-state": "^1.0.0",
    "@types/prosemirror-test-builder": "^1.0.0",
    "@types/prosemirror-view": "^1.0.0",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@zeit/next-css": "^1.0.1",
    "coveralls": "^3.0.9",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "firebase": "^7.4.0",
    "github-markdown-css": "^4.0.0",
    "immer": "^5.3.2",
    "jest": "^25.0.0",
    "jest-extended": "^0.11.4",
    "jest-puppeteer": "^4.3.0",
    "jest-puppeteer-istanbul": "*",
    "jest-transform-stub": "^2.0.0",
    "lodash": "^4.17.13",
    "markdown-it": "^10.0.0",
    "next": "^9.2.0",
    "next-images": "^1.3.0",
    "prettier": "^1.19.1",
    "prosemirror-commands": "^1.0.8",
    "prosemirror-dev-tools": "^2.1.1",
    "prosemirror-dropcursor": "^1.1.1",
    "prosemirror-history": "^1.0.4",
    "prosemirror-inputrules": "^1.0.4",
    "prosemirror-keymap": "^1.0.1",
    "prosemirror-model": "^1.7.1",
    "prosemirror-schema-list": "^1.0.3",
    "prosemirror-state": "^1.2.3",
    "prosemirror-tables": "^1.0.0",
    "prosemirror-test-builder": "^1.0.0",
    "prosemirror-view": "^1.14.2",
    "puppeteer": "^2.0.0",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "~3.7.2",
    "unstated-next": "^1.1.0"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 12"
  },
  "//": [
    "jest-puppeteer: Requires Jest v22+ TypeScript users should additionally install `@types/puppeteer`, `@types/jest-environment-puppeteer` and `@types/expect-puppeteer`"
  ]
}