{
    "name": "@rino.app/web",
    "version": "0.9.1",
    "license": "GPL-3.0",
    "scripts": {
        "dev": "next dev",
        "start": "next start",
        "build": "next build",
        "export": "next export -o dist",
        "test": "jest --runInBand",
        "analyze": "ANALYZE=true next build",
        "lint": "eslint --ignore-path .gitignore --ext '.js,.jsx,.ts,.tsx'"
    },
    "renovate": {
        "extends": [
            "config:base",
            ":semanticCommitTypeAll(chore)"
        ],
        "postUpdateOptions": [
            "yarnDedupeFewer"
        ],
        "lockFileMaintenance": {
            "enabled": true,
            "extends": "schedule:monthly"
        },
        "packageRules": [
            {
                "depTypeList": [
                    "dependencies"
                ],
                "updateTypes": [
                    "patch",
                    "minor"
                ],
                "groupName": "dependencies (non-major)"
            }
        ],
        "ignoreDeps": [
            "node"
        ],
        "rangeStrategy": "update-lockfile"
    },
    "husky": {
        "hooks": {
            "pre-commit": "./scripts/pre-commit.sh"
        }
    },
