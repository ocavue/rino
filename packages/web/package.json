{
    "name": "@rino.app/web",
    "version": "0.10.1",
    "license": "GPL-3.0",
    "scripts": {
        "dev": "next dev",
        "start": "next start",
        "build": "next build",
        "build:analyze": "ANALYZE=true next build",
        "export": "next export -o dist"
    },
    "renovate": {
        "extends": [
            "config:base",
            ":semanticCommitTypeAll(chore)"
        ],
        "postUpdateOptions": [
            "yarnDedupeFewer"
        ],
        "lockFileMaintenance": {
            "enabled": true,
            "extends": "schedule:monthly"
        },
        "packageRules": [
            {
                "depTypeList": [
                    "dependencies"
                ],
                "updateTypes": [
                    "patch",
                    "minor"
                ],
                "groupName": "dependencies (non-major)"
            }
        ],
        "ignoreDeps": [
            "@changesets/cli",
            "node"
        ],
        "rangeStrategy": "update-lockfile"
    },
    "dependencies": {
        "@changesets/cli": "~2.7.0",
        "@lhci/cli": "^0.4.0",
        "@material-ui/core": "^4.8.2",
        "@material-ui/icons": "^4.5.1",
        "@next/bundle-analyzer": "^9.2.0",

        "@testing-library/react": "^10.0.0",
        "@types/jest-environment-puppeteer": "^4.3.0",
        "@types/lodash": "^4.14.134",
        "@types/markdown-it": "^10.0.0",
        "@types/node": "^12.0.0",

        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "babel-plugin-inline-react-svg": "^1.1.1",
        "commonmark-spec": "^0.29.0",
        "dotenv": "^8.2.0",
        "firebase": "^7.4.0",
        "firebase-admin": "^8.10.0",
        "firebase-tools": "^8.0.0",
        "immer": "^6.0.0",
        "jest-extended": "^0.11.4",

        "jest-puppeteer": "^4.3.0",
        "jest-puppeteer-istanbul": "^0.5.2",

        "lodash": "^4.17.13",
        "markdown-it": "^11.0.0",
        "next": "^9.0.0",
        "next-images": "^1.3.0",
        "next-offline": "^5.0.0",

        "puppeteer": "^3.3.0",
        "raw-loader": "^4.0.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-test-renderer": "^16.0.0",
        "sass": "^1.26.7",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "unstated-next": "^1.1.0",
        "webpack": "^4.0.0"
    },
    "browserslist": [
        "> 1%",
        "not dead",
        "not op_mini all"
    ],
    "engines": {
        "node": ">= 12"
    },
    "files": [
        "this_file_dose_not_exist"
    ],
    "publishConfig": {
        "access": "public"
    }
}
