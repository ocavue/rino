    "license": "GPL-3.0",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build:esm": "tsc -p tsconfig.json --target es6 --outDir dist/esm --module es2015",
