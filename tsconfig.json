{
    "compilerOptions": {
        "target": "es6",
        "strict": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "dist",
        ".next",
        "coverage"
    ],
}